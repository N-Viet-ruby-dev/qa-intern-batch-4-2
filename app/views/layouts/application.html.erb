<!DOCTYPE html>
<html>
  <head>
    <title><%= full_title(yield(:title)) %></title>
    <%= render "layouts/rails_default" %>
  </head>
  <body>
    <%= render "layouts/flash_messages" %>
    <div class="container-fluid">
      <%= render "layouts/header" %>
      <div class="row">
        <%= render "layouts/left" %>
        <%= yield %>
        <%= render "layouts/right" %>
      </div>
    </div>
  </body>
</html>
